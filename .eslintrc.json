{
    "extends":"eslint:recommended",
    "env": {
      "node": true,
      "jasmine": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
      "no-console":0,
      "no-useless-escape":0,
      "prefer-arrow-callback": ["error", { "allowNamedFunctions": true }]
    }
}
